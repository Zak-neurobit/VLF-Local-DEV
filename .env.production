# Production configuration for Vercel deployment
# These values should be overridden by Vercel environment variables

# Build Configuration
SKIP_BUILD_STATIC_GENERATION=true
NEXT_TELEMETRY_DISABLED=1

# Database
DATABASE_URL="postgresql://dummy:dummy@localhost:5432/dummy"

# Authentication
NEXTAUTH_SECRET="dummy-secret-for-build-must-be-at-least-32-chars-long"
NEXTAUTH_URL="https://vasquez-law-website.vercel.app"
GOOGLE_CLIENT_ID="dummy-client-id-for-build"
GOOGLE_CLIENT_SECRET="dummy-client-secret-for-build"

# Basic Configuration
NODE_ENV="production"
NEXT_PUBLIC_APP_URL="https://vasquez-law-website.vercel.app"
NEXT_PUBLIC_BASE_URL="https://vasquez-law-website.vercel.app"

# Email - empty to avoid connection attempts during build
SMTP_HOST=""
SMTP_PORT=""
SMTP_USER=""
SMTP_PASSWORD=""
SMTP_FROM=""

# Optional services - empty to avoid connection attempts
REDIS_URL=""
OPENAI_API_KEY="sk-dummy-key-for-build-only"

# Disable services during build
DISABLE_REDIS=true
DISABLE_BULL=true
DISABLE_EMAIL_SERVICE=true
MOCK_REDIS=true
