# AWS Infrastructure
AWS_ACCOUNT_ID=your-account-id
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key

# S3 Buckets
AWS_S3_BUCKET=vasquezlaw-static
AWS_S3_DOCUMENTS_BUCKET=vasquezlaw-documents
AWS_S3_BACKUPS_BUCKET=vasquezlaw-backups

# CloudFront
CLOUDFRONT_DISTRIBUTION_ID=your-distribution-id
CLOUDFRONT_DOMAIN=d1234567890.cloudfront.net

# ECS Configuration
ECR_REPO=your-account.dkr.ecr.us-east-1.amazonaws.com/vasquezlaw
ECS_CLUSTER=vasquezlaw-cluster
ECS_SERVICE=vasquezlaw-web
ECS_AGENTS_SERVICE=vasquezlaw-agents

# RDS Database
DATABASE_URL=postgresql://admin:password@vasquezlaw-prod.region.rds.amazonaws.com:5432/vasquezlaw

# ElastiCache Redis
REDIS_HOST=vasquezlaw-redis.cache.amazonaws.com
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password

# Application URLs
NEXT_PUBLIC_APP_URL=https://vasquezlawnc.com
NEXT_PUBLIC_API_URL=https://api.vasquezlawnc.com
NEXTAUTH_URL=https://vasquezlawnc.com

# Authentication
NEXTAUTH_SECRET=your-nextauth-secret-generate-with-openssl

# AI Services
OPENAI_API_KEY=sk-your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key

# CrewAI Configuration
CREWAI_API_KEY=your-crewai-key
CREWAI_WEBHOOK_URL=https://api.vasquezlawnc.com/api/webhooks/crewai
CREWAI_DATABASE_INTEGRATION=enabled
CREWAI_GHL_INTEGRATION=enabled
CREWAI_PARALLEL_TASKS=10

# Retell AI (Voice Agents)
RETELL_API_KEY=your-retell-api-key
RETELL_AGENT_ID_ENGLISH=agent-id-english
RETELL_AGENT_ID_SPANISH=agent-id-spanish
RETELL_WEBHOOK_URL=https://api.vasquezlawnc.com/api/webhooks/retell

# GoHighLevel CRM
GHL_API_KEY=your-ghl-api-key
GHL_LOCATION_ID=your-location-id
GHL_PIPELINE_ID=your-pipeline-id
GHL_WEBHOOK_URL=https://api.vasquezlawnc.com/api/webhooks/ghl

# Google Services
GOOGLE_MAPS_API_KEY=your-google-maps-key
GOOGLE_PLACES_API_KEY=your-google-places-key
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_MY_BUSINESS_API_KEY=your-gmb-key

# Email Services
EMAIL_SERVER_HOST=smtp.office365.com
EMAIL_SERVER_PORT=587
EMAIL_SERVER_USER=noreply@vasquezlawnc.com
EMAIL_SERVER_PASSWORD=your-email-password
EMAIL_FROM=Vasquez Law Firm <noreply@vasquezlawnc.com>

# Payment Processing
STRIPE_SECRET_KEY=sk_live_your-stripe-key
STRIPE_PUBLISHABLE_KEY=pk_live_your-stripe-key
LAWPAY_MERCHANT_ID=your-lawpay-id
LAWPAY_API_KEY=your-lawpay-key

# Monitoring
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
SENTRY_AUTH_TOKEN=your-sentry-auth-token
NEW_RELIC_LICENSE_KEY=your-new-relic-key

# Feature Flags
ENABLE_AI_CHAT=true
ENABLE_VOICE_AGENTS=true
ENABLE_CREWAI_AGENTS=true
ENABLE_COMPETITOR_MONITORING=true
ENABLE_CONTENT_FACTORY=true
ENABLE_REVIEW_HARVESTING=true

# Security
RATE_LIMIT_PER_MINUTE=100
ALLOWED_ORIGINS=https://vasquezlawnc.com,https://www.vasquezlawnc.com
JWT_SECRET=your-jwt-secret
ENCRYPTION_KEY=your-32-character-encryption-key

# AWS Secrets Manager (Alternative to env vars)
USE_AWS_SECRETS=true
AWS_SECRETS_NAME=vasquezlaw/production/secrets