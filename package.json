  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "dev": "node scripts/unified-dev.js",
    "dev:minimal": "node scripts/unified-dev.js --minimal",
    "dev:mock": "node scripts/unified-dev.js --mock",
    "build": "node scripts/unified-build.js",
    "build:static": "node scripts/unified-build.js --static",
    "build:deploy": "node scripts/unified-build.js --deploy",
    "build:analyze": "node scripts/unified-build.js --analyze",
    "start": "next start",
    "start:verify": "tsx scripts/verify-deployment.ts && next start",
    "deploy": "node scripts/unified-deploy.js",
    "deploy:aws": "node scripts/unified-deploy.js --aws",
    "deploy:agents": "node scripts/unified-deploy.js --agents",
    "test": "node scripts/unified-test.js",
    "test:watch": "node scripts/unified-test.js --watch",
    "test:e2e": "node scripts/unified-test.js --e2e",
    "test:api": "node scripts/unified-test.js --api",
    "test:local": "node scripts/unified-test.js --local",
    "db:setup": "prisma db push && prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:health": "tsx scripts/test-db-connection.ts",
    "crews": "node scripts/unified-crews.js",
    "crews:dev": "node scripts/unified-crews.js --dev",
    "lint": "next lint && eslint . --ext .ts,.tsx",
    "clean": "node scripts/unified-clean.js",
    "setup": "tsx scripts/setup-environment.js && prisma generate",
    "validate": "tsx scripts/validate-env.ts",
    "seo": "tsx scripts/start-seo-automation.ts",
    "content:import": "tsx scripts/import-content.ts",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "prepare": "node -e \"if (process.env.NODE_ENV !== 'production' && !process.env.CI) { require('child_process').execSync('husky', {stdio: 'inherit'}) }\"",
    "pre-commit": "pnpm run lint && pnpm run type-check && pnpm test",
    "postinstall": "pnpm exec prisma generate && node scripts/fix-winston-dependencies-pnpm.js && node scripts/patch-pdf-parse.js",
    "preinstall": "npx only-allow pnpm",
    "setup:partytown": "node scripts/setup-partytown.js",
    "build:socket": "tsc --project tsconfig.socket.json && mkdir -p dist/lib/socket && cp src/lib/socket/server-setup.js dist/lib/socket/ && cp dist/lib/socket/server.js dist/lib/socket/ 2>/dev/null || true",
    "prisma:generate": "prisma generate"
  },
